Entrar

<?php
defined('BASEPATH') OR exit('Sem acesso ao arquivo direto');

class Entrar extends CI_Controller
{
    public function index()
    {
        $parametros['titulo'] = 'aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa ';
        $parametros['subtitulo'] = 'aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa ';

        $dadostela['dados'] = $parametros;
        $dadostela['view'] = 'vEntrar';
        $this->load->view('vLayout',$dadostela);
        
    }
} 

