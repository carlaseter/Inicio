Nome: Notas(Enquanto, VISUALG)

Algoritmo "semnome"
//  
//       terminar cod errado
// Descrição   : Aqui você descreve o que o programa faz! (função)
// Autor(a)    : Nome do(a) aluno(a)
// Data atual  : 09/04/2019
Var
// Seção de Declarações das variáveis 
aluno: inteiro
nota: real
total: real
media: real
aluno1: real
aluno2: real
aluno3: real
aluno4: real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
aluno:=1
nota:=0
total:=0
media:=0
   enquanto aluno <=4 faca
   escreva ("digite primeira nota aluno",aluno)
   leia (nota)
   total:= total+nota
    escreva ("digite segunda nota aluno",aluno)
   leia (nota)
   total:= total+nota
   media:=soma/2
   escolha aluno
   caso 1
   aluno1:=media
    caso 2
   aluno2:=media
    caso 3
   aluno3:=media
    caso 4
   aluno4:=media
   fimescolha
   aluno+1
  fimenquanto
  
  escreval("nota aluno 1: ",aluno1)
  se aluno1 >=7 entao ("aprovado")
  se nao
  escreval ("reprovado")
  fimse
  escreval("nota aluno 2: ",aluno1)
  se aluno1 >=7 entao ("aprovado")
  se nao
  escreval ("reprovado")
  fimse
  escreval("nota aluno 3: ",aluno1)
  se aluno1 >=7 entao ("aprovado")
  se nao
  escreval ("reprovado")
  fimse
  escreval("nota aluno 4: ",aluno1)
  se aluno1 >=7 entao ("aprovado")
  se nao
  escreval ("reprovado")
  fimse

