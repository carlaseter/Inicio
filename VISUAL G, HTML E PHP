///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
Nome: PHP Inicio

<?php
defined('BASEPATH') OR exit('Sem acesso ao arquivo direto');

class OlaMundo extends CI_Controller 
{
	public function index()
	{
		echo "Olá Mundo";
	}
	public function somar($par1 = 0, $par2 = 0)
	{
		$resultado = $par1 +$par2;
		echo "Resultado encontrado: ". $resultado;
	}
	public function tabuada($tabuada, $ate = 10)
	{
		for($i=1; $i <= $ate; $i++)
		{
			echo $i;
			echo "x";
			echo $tabuada;
			echo "=";
			echo $i * $tabuada;
			echo "<br />";
		}
	}

}
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
Nome: PHP Projeto 1

OlaMundo

<?php
defined('BASEPATH') OR exit('Sem acesso ao arquivo direto');

class OlaMundo extends CI_Controller 
{
	public function index()
	{
		echo "Olá Mundo";
	}
	public function somar($par1 = 0, $par2 = 0)
	{
		$resultado = $par1 +$par2;
		echo "Resultado encontrado: ". $resultado;
	}
	public function tabuada($tabuada, $ate = 10)
	{
		for($i=1; $i <= $ate; $i++)
		{
			echo $i;
			echo "x";
			echo $tabuada;
			echo "=";
			echo $i * $tabuada;
			echo "<br />";
		}
	}

}
